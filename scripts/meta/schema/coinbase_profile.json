{
	"api": "coinbase",
	"model": "profile",
	"modelDescription": "Profile represents a profile to interact with the API.",
	"endpoints": [
		{
			"path": "/profiles",
			"httpMethod": "GET",
			"enumRoot": "Profiles",
			"slice": true,
			"description": "Gets a list of all of the current user's profiles.",
			"queryParams": [
				{
					"identifier": "active",
					"goType": "bool"
				}
			]
		},
		{
			"path": "/profiles",
			"httpMethod": "POST",
			"enumRoot": "CreateProfile",
			"description": "CreateProfile will create a new profile. Will fail if no name is provided or if user already has max number of profiles.",
			"body": [
				{
					"identifier": "name",
					"goType": "string"
				}
			]
		},
		{
			"path": "/profiles/transfer",
			"httpMethod": "POST",
			"enumRoot": "CreateProfileTransfer",
			"returnType": "none",
			"description": "CreateProfileTransfer will transfer an amount of currency from one profile to another.  This endpoint requires the \"transfer\" permission.",
			"body": [
				{
					"identifier": "from",
					"goType": "string"
				},
				{
					"identifier": "to",
					"goType": "string"
				},
				{
					"identifier": "currency",
					"goType": "string"
				},
				{
					"identifier": "amount",
					"goType": "string"
				}
			]
		}
	],
	"modelFields": [
		{
			"identifier": "id",
			"goType": "string"
		},
		{
			"identifier": "user_id",
			"goType": "string"
		},
		{
			"identifier": "name",
			"goType": "string"
		},
		{
			"identifier": "active",
			"goType": "bool"
		},
		{
			"identifier": "is_default",
			"goType": "bool"
		},
		{
			"identifier": "has_margin",
			"goType": "bool"
		},
		{
			"identifier": "created_at",
			"goType": "time.Time"
		}
	]
}
